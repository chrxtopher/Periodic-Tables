    "browserlist": "^1.0.1",
    "browserslist": "^4.19.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "follow-redirects": "^1.14.9",
    "glob-parent": "^6.0.2",
    "immer": "^9.0.12",
    "json-schema": "^0.4.0",
    "knex": "^1.0.2",
    "nanoid": "^3.3.1",
    "node-fetch": "^3.2.1",
    "node-forge": "^1.2.1",
    "normalize-url": "^7.0.3",
